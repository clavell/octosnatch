pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
pushfactor=0 fmag=1 topv=.75 sinkv=.5
maxpush=25 drag=0.9
cls()

wavetimer=0
octotimer=0
music (o)

octo={}
octo.x=64
octo.y=4
octo.vx=0
octo.vy=0
octo.ax=0
octo.ay=0
octo.refl=false
octo.bub=false
octo.snatch=false

bub={}
bub.x=-30
bub.y=-30
bub.time=0
bub.delta=0.1

---- globals ----
function octocount()
	if (octotimer == 30) then
	                 octotimer=0
	                 wavetimer+=2
	else	
																		octotimer+=1
	end
	if (wavetimer==16) wavetimer=0
end

---- display ----
-- octo
function legsdown()
	spr(2,octo.x,octo.y,2,3,octo.refl)
end

function legsmidup()
	spr(0,octo.x,octo.y,2,3,octo.refl)
end

function legsup()
	spr(4,octo.x-8,octo.y,4,3,octo.refl)
end

function legsnatch()
	spr(64,octo.x-8,octo.y,4,3,octo.refl)
end

function bubble()
	t=time()-bub.time
	for i=1,4 do
		if(t<bub.delta*i) then
			--sspr(56+(i*8),16,8,8,bub.x-4,bub.y+16,16,16)
			--sspr(56+(i*8),16,8,8,bub.x+4,bub.y+16,16,16)
	 	spr(i+39,bub.x-1,bub.y+18,2,1)
			spr(i+39,bub.x+1,bub.y+18,2,1,true)
			spr(i+39,bub.x-5,bub.y+24,3,1)
			spr(i+39,bub.x-3,bub.y+24,3,1,true)
			break
		end
	end
end

function octosplay()
	bubble()
	-- pushing overrides snatching
	if(btn(5)) then
		if(pushfactor<9) then legsmidup()
		else legsup()
		end
		octo.bub=true
	else
		if(btn(4)) then
			legsnatch()
		else
			legsdown()
		end
		
		--if just released push, start bubbles
		if(octo.bub==true) then
			bub.x=octo.x
			bub.y=octo.y
			bub.time=time()
			octo.bub=false
		end
	end
end

---- movement ----
-- octo --
function octosnatch()
	if(btn(4)) then
				octo.snatch=true
	else
			octo.snatch=false	                 
	end
end

function octosink()
	if (octotimer == 1)
	then	octo.vy+=sinkv
	end
end

function octopush()
	if(btn(5)) then
				pushfactor=min(pushfactor+1,maxpush)
	else
			if(pushfactor ~= 0) then
			          sfx(3,3)
			     octo.vy-=pushfactor*.2
			     pushfactor=0
			end	                 
	end
end

function octomove()
	octosink()
	--arrow key movement
	if(btn(0)) octo.x-=1 octo.refl=true
	if(btn(1)) octo.x+=1 octo.refl=false
	if(btn(3)) octo.vy+=fmag
	octopush()
		--limit speed
	octo.vy = min(octo.vy,topv)
	octo.vy = drag*octo.vy
	octo.x+=octo.vx
	octo.y+=octo.vy
end

function checkedges()
	if(octo.x<0) octo.x=0
	if(octo.x>(128-16)) octo.x=(128-16)
	if(octo.y<0) octo.y=0
	if(octo.y>(128-24)) octo.y=(128-24)
end

function wave()
	x=-16
	while(x<128) do
		spr(8,x+wavetimer,0,2,2)
		x+=16
	end
end

---- main ----
function _update()
	octomove()
	checkedges()
	octocount()
end

function _draw()
	rectfill(0,16,128,128,color(12))
	map(0,0,0,0,16,32)
	wave()
	octosplay()
end
